{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/clients", "destination": "/clients.html" },
    { "source": "/policies", "destination": "/policies.html" },
    { "source": "/claims", "destination": "/claims.html" },
    { "source": "/commissions", "destination": "/commissions.html" },
    { "source": "/reports", "destination": "/reports.html" },
    { "source": "/settings", "destination": "/settings.html" },
    { "source": "/change-password", "destination": "/change-password.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
